<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Covid19App">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Covid19App.userProductList"></activity>
        <activity android:name="com.example.Covid19App.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Covid19App.MainStoreActivity" />
        <activity android:name="com.example.Covid19App.productUpdateTest" />
        <activity android:name="com.example.Covid19App.StoreProductUpdateActivity" />
        <activity android:name="com.example.Covid19App.StoreList" />
        <activity android:name="com.example.Covid19App.MainActivity" />
    </application>

</manifest>